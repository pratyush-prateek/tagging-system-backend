{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/item-tagger-service/src",
  "monorepo": true,
  "root": "apps/item-tagger-service",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/item-tagger-service/tsconfig.app.json"
  },
  "projects": {
    "item-tagger-service": {
      "type": "application",
      "root": "apps/item-tagger-service",
      "entryFile": "main",
      "sourceRoot": "apps/item-tagger-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/item-tagger-service/tsconfig.app.json",
        "webpackConfigPath": "apps/item-tagger-service/webpack.config.js"
      }
    },
    "item-retrieval-service": {
      "type": "application",
      "root": "apps/item-retrieval-service",
      "entryFile": "main",
      "sourceRoot": "apps/item-retrieval-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/item-retrieval-service/tsconfig.app.json",
        "webpackConfigPath": "apps/item-retrieval-service/webpack.config.js"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "item-tag-db-update-consumer": {
      "type": "application",
      "root": "apps/item-tag-db-update-consumer",
      "entryFile": "main",
      "sourceRoot": "apps/item-tag-db-update-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/item-tag-db-update-consumer/tsconfig.app.json",
        "webpackConfigPath": "apps/item-tag-db-update-consumer/webpack.config.js"
      }
    }
  }
}